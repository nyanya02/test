package jp.co.internous.action;

public class Main {

	public static void main(String[] args){
		System.out.println(goukei(3, 4));
		System.out.println(Calcurate(5, 3.14));
		System.out.println(hikizan(5, 1));
		System.out.println(kakezan(5, 1));
		System.out.println(warizan(5, 1));
	}

	public static int goukei(int number1, int number2){
		return number1 + number2;
	}

	public static double Calcurate(int radius, double rate){
		return radius * radius * rate;
	}

	public static int hikizan(int num1, int num2){
		return num1 - num2;
	}

	public static int kakezan(int num1, int num2){
		return num1 * num2;
	}

	public static int warizan(int num1, int num2){
		return num1 / num2;
	}
}
