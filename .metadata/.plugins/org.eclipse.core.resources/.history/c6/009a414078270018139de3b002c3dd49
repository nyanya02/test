
public class HelloWorld {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

		int a = 0;

		switch(a){
		case 0:
			System.out.println("0");
			break;

		case 1:
			System.out.println("1");
			break;

		case 2:
			System.out.println("2");
			break;

			default:
				System.out.println("all");
		}
	}

}
